<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.ray.app.workflow.select.action" extends="default" namespace="/module/app/system/workflow/select">
		<action name="select_*" class="com.ray.app.workflow.select.action.SelectAction" method="{1}">
			<result name="selowner" type="freemarker">/com/ray/app/workflow/select/view/selowner.ftl</result>
			<result name="selreader" type="freemarker">/com/ray/app/workflow/select/view/selreader.ftl</result>
			<result name="form" type="freemarker">/com/ray/app/workflow/select/view/form.ftl</result>
			
			<result name="selectperson" type="freemarker">/com/ray/app/workflow/select/view/selectperson.ftl</result>
			<result name="selectrole" type="freemarker">/com/ray/app/workflow/select/view/selectrole.ftl</result>
			<result name="selectworkgroup" type="freemarker">/com/ray/app/workflow/select/view/selectworkgroup.ftl</result>
			<result name="selectdept" type="freemarker">/com/ray/app/workflow/select/view/selectdept.ftl</result>
		</action>
	</package>
</struts>